{
  "title": "视觉自动巡检机器人用户手册",
  "description": "视觉巡检机器人即利用视觉导航程序进行导航巡检的机器人，本手册为其使用手册。包含了地图的创建，路径的绘制和导航的控制",
  "language": "zh",
  "gitbook": "3.x.x",
  "plugins": [
    "github",
    "editlink",
    "prism",
    "downloadpdf",
    "yahei",
    "katex",
    "search",
    "include-codeblock",
    "anchor-navigation-ex",
    "randoms-versions",
    "-live-reload"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/BlueWhaleRobot/galileo-doc"
    },
    "editlink": {
      "base": "https://github.com/BlueWhaleRobot/galileo-doc/edit/master",
      "label": "编辑本页"
    },
    "sharing": {
      "weibo": true,
      "twitter": false,
      "facebook": false
    },
    "downloadpdf": {
      "base": "https://doc.bwbot.org/pdf/galileo-doc",
      "label": "下载PDF",
      "multilingual": false
    },
    "fontSettings": {
      "theme": "white",
      "family": "msyh",
      "size": 2
    },
    "versions": {
      "host": "https://galileo-doc.bwbot.org"
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 14,
    "paperSize": "a4",
    "margin": {
      "right": 30,
      "left": 30,
      "top": 30,
      "bottom": 50
    }
  }
}
